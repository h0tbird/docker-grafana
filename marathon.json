{
  "id": "/sys/grafana",
  "instances": 1,
  "cpus": 0.5,
  "mem": 2048,
  "disk": 0,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "labels": {
    "owner": "sys"
  },
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "grafana/grafana",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 3000, "hostPort": 0 }
      ],
      "privileged": false,
      "parameters": [
        { "key": "env", "value": "GF_SECURITY_ADMIN_PASSWORD" }
      ],
      "forcePullImage": false
    }
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 120,
      "intervalSeconds": 30,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "protocol": "TCP",
      "timeoutSeconds": 5
    }
  ],
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 1
  }
}
